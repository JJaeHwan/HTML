	$(function() {
		
		xhr = new XMLHttpRequest();
		
		$('#btn1').on('click', function() {
			
// 			location.href="memberList.jsp";
			// 비동기 방식으로 요청
			xhr.open('get', 'memberList.jsp', true);
			xhr.send();
			
		})
		
	})

</script>

